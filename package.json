{
	"name": "remixpluginmngr",
	"displayName": "remixPluginMngr",
	"description": "Remix Plugin Manager for VSCode",
	"version": "0.0.1",
	"publisher": "mathcody",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:RemiXPluginMngr"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "remixPluginMngr.refreshEntry",
				"title": "Refresh",
				"icon": "assets/reload.svg"
			},
			{
				"command": "remixPluginMngr.addEntry",
				"title": "Add"
			},
			{
				"command": "remixPluginMngr.editEntry",
				"title": "Edit"
			},
			{
				"command": "remixPluginMngr.deleteEntry",
				"title": "Delete"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "remix-plugin-mngr",
					"title": "Remix Plugin manager",
					"icon": "assets/remixplugin.svg"
				}
			]
		},
		"views": {
			"remix-plugin-mngr": [
				{
					"id": "remixPluginMngr",
					"name": "Plugins"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"eslint-config-airbnb": "^17.1.1",
		"eslint-config-prettier": "^6.0.0",
		"eslint-plugin-prettier": "^3.1.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"prettier": "^1.18.2",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.0-next.0"
	}
}
